{
  "name": "efoft-ssl",
  "author": "efoft",
  "version": "0.0.5",
  "summary": "Puppet module managning self sigend certs/DH params",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/efoft/puppet-ssl.git",
  "project_page": "https://github.com/efoft/puppet-ssl",
  "description": "Small SSL certificate handling modules",
  "tags": [
    "ssl",
    "openssl",
    "apache",
    "nginx",
    "webserver",
    "certificate"
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.2.1"
    }
  ]
}
